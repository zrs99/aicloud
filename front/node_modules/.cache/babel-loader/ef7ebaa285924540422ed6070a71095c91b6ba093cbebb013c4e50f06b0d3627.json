{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted, onUnmounted } from 'vue';\nexport default {\n  setup() {\n    const loadedImages = ref([]);\n    const loading = ref(false);\n    const currentPage = ref(0);\n    const totalPages = 10; // 假设PDF有10页\n\n    // 模拟获取PDF页面的图片\n    const getPageImage = pageNumber => {\n      return new Promise(resolve => {\n        setTimeout(() => {\n          resolve(`/path-to-pdf-images/page-${pageNumber}.jpg`);\n        }, 500);\n      });\n    };\n    const loadMorePages = async () => {\n      if (loading.value || currentPage.value >= totalPages) return;\n      loading.value = true;\n      const pagesToLoad = Math.min(3, totalPages - currentPage.value);\n      const newImages = [];\n      for (let i = 1; i <= pagesToLoad; i++) {\n        const image = await getPageImage(currentPage.value + i);\n        newImages.push(image);\n      }\n      loadedImages.value = [...loadedImages.value, ...newImages];\n      currentPage.value += pagesToLoad;\n      loading.value = false;\n    };\n    const handleScroll = () => {\n      const scrollPosition = window.scrollY + window.innerHeight;\n      const documentHeight = document.documentElement.scrollHeight;\n      if (documentHeight - scrollPosition < 500) {\n        loadMorePages();\n      }\n    };\n    onMounted(async () => {\n      await loadMorePages(); // 初始加载\n      window.addEventListener('scroll', handleScroll);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n    });\n    return {\n      loadedImages,\n      loading\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","setup","loadedImages","loading","currentPage","totalPages","getPageImage","pageNumber","Promise","resolve","setTimeout","loadMorePages","value","pagesToLoad","Math","min","newImages","i","image","push","handleScroll","scrollPosition","window","scrollY","innerHeight","documentHeight","document","documentElement","scrollHeight","addEventListener","removeEventListener"],"sources":["D:\\Study\\Code\\AICloud\\front\\src\\components\\PDFViewer.vue"],"sourcesContent":["<template>\r\n  <div class=\"pdf-container\">\r\n    <img\r\n      v-for=\"(image, index) in loadedImages\"\r\n      :key=\"index\"\r\n      :src=\"image\"\r\n      class=\"pdf-page\"\r\n    />\r\n    <div v-if=\"loading\" class=\"loading\">加载中...</div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, onUnmounted } from 'vue'\r\n\r\nexport default {\r\n  setup() {\r\n    const loadedImages = ref([])\r\n    const loading = ref(false)\r\n    const currentPage = ref(0)\r\n    const totalPages = 10 // 假设PDF有10页\r\n\r\n    // 模拟获取PDF页面的图片\r\n    const getPageImage = (pageNumber) => {\r\n      return new Promise((resolve) => {\r\n        setTimeout(() => {\r\n          resolve(`/path-to-pdf-images/page-${pageNumber}.jpg`)\r\n        }, 500)\r\n      })\r\n    }\r\n\r\n    const loadMorePages = async () => {\r\n      if (loading.value || currentPage.value >= totalPages) return\r\n\r\n      loading.value = true\r\n      const pagesToLoad = Math.min(3, totalPages - currentPage.value)\r\n      const newImages = []\r\n\r\n      for (let i = 1; i <= pagesToLoad; i++) {\r\n        const image = await getPageImage(currentPage.value + i)\r\n        newImages.push(image)\r\n      }\r\n\r\n      loadedImages.value = [...loadedImages.value, ...newImages]\r\n      currentPage.value += pagesToLoad\r\n      loading.value = false\r\n    }\r\n\r\n    const handleScroll = () => {\r\n      const scrollPosition = window.scrollY + window.innerHeight\r\n      const documentHeight = document.documentElement.scrollHeight\r\n\r\n      if (documentHeight - scrollPosition < 500) {\r\n        loadMorePages()\r\n      }\r\n    }\r\n\r\n    onMounted(async () => {\r\n      await loadMorePages() // 初始加载\r\n      window.addEventListener('scroll', handleScroll)\r\n    })\r\n\r\n    onUnmounted(() => {\r\n      window.removeEventListener('scroll', handleScroll)\r\n    })\r\n\r\n    return {\r\n      loadedImages,\r\n      loading\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.pdf-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  gap: 20px;\r\n  padding: 20px;\r\n}\r\n\r\n.pdf-page {\r\n  max-width: 100%;\r\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.loading {\r\n  text-align: center;\r\n  padding: 20px;\r\n}\r\n</style>"],"mappings":";AAaA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAU,QAAS,KAAI;AAEhD,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,YAAW,GAAIJ,GAAG,CAAC,EAAE;IAC3B,MAAMK,OAAM,GAAIL,GAAG,CAAC,KAAK;IACzB,MAAMM,WAAU,GAAIN,GAAG,CAAC,CAAC;IACzB,MAAMO,UAAS,GAAI,EAAC,EAAE;;IAEtB;IACA,MAAMC,YAAW,GAAKC,UAAU,IAAK;MACnC,OAAO,IAAIC,OAAO,CAAEC,OAAO,IAAK;QAC9BC,UAAU,CAAC,MAAM;UACfD,OAAO,CAAC,4BAA4BF,UAAU,MAAM;QACtD,CAAC,EAAE,GAAG;MACR,CAAC;IACH;IAEA,MAAMI,aAAY,GAAI,MAAAA,CAAA,KAAY;MAChC,IAAIR,OAAO,CAACS,KAAI,IAAKR,WAAW,CAACQ,KAAI,IAAKP,UAAU,EAAE;MAEtDF,OAAO,CAACS,KAAI,GAAI,IAAG;MACnB,MAAMC,WAAU,GAAIC,IAAI,CAACC,GAAG,CAAC,CAAC,EAAEV,UAAS,GAAID,WAAW,CAACQ,KAAK;MAC9D,MAAMI,SAAQ,GAAI,EAAC;MAEnB,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,IAAKJ,WAAW,EAAEI,CAAC,EAAE,EAAE;QACrC,MAAMC,KAAI,GAAI,MAAMZ,YAAY,CAACF,WAAW,CAACQ,KAAI,GAAIK,CAAC;QACtDD,SAAS,CAACG,IAAI,CAACD,KAAK;MACtB;MAEAhB,YAAY,CAACU,KAAI,GAAI,CAAC,GAAGV,YAAY,CAACU,KAAK,EAAE,GAAGI,SAAS;MACzDZ,WAAW,CAACQ,KAAI,IAAKC,WAAU;MAC/BV,OAAO,CAACS,KAAI,GAAI,KAAI;IACtB;IAEA,MAAMQ,YAAW,GAAIA,CAAA,KAAM;MACzB,MAAMC,cAAa,GAAIC,MAAM,CAACC,OAAM,GAAID,MAAM,CAACE,WAAU;MACzD,MAAMC,cAAa,GAAIC,QAAQ,CAACC,eAAe,CAACC,YAAW;MAE3D,IAAIH,cAAa,GAAIJ,cAAa,GAAI,GAAG,EAAE;QACzCV,aAAa,CAAC;MAChB;IACF;IAEAZ,SAAS,CAAC,YAAY;MACpB,MAAMY,aAAa,CAAC,GAAE;MACtBW,MAAM,CAACO,gBAAgB,CAAC,QAAQ,EAAET,YAAY;IAChD,CAAC;IAEDpB,WAAW,CAAC,MAAM;MAChBsB,MAAM,CAACQ,mBAAmB,CAAC,QAAQ,EAAEV,YAAY;IACnD,CAAC;IAED,OAAO;MACLlB,YAAY;MACZC;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}